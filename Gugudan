import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner scan = new Scanner(System.in);
		
		int num = scan.nextInt();
		int i = 1;
		int res;
		
		while(i<=9) {
			
			if(i<=9) {
			res = num*i;
			System.out.println(num+" * "+i+" = "+res);
			}
			i++;
			
						
		}
	}

}
